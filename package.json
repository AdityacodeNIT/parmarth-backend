{
  "name": "adyog-ecommerce-backend",
  "version": "1.0.0",
  "description": "Adyog E-commerce Platform Backend API",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon -r dotenv/config --experimental-json-modules src/index.js",
    "start": "node -r dotenv/config --experimental-json-modules src/index.js",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write src/**/*.js",
    "format:check": "prettier --check src/**/*.js",
    "test": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "prepare": "husky install"
  },
  "keywords": [
    "javascript",
    "backend",
    "ecommerce",
    "nodejs",
    "express",
    "mongodb"
  ],
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "author": "Aditya",
  "license": "MIT",
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "mongodb-memory-server": "^9.1.6",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "@tanstack/react-query": "^5.81.5",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.0.3",
    "compression": "^1.8.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.3",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "helmet": "^8.1.0",
    "hpp": "^0.2.3",
    "install": "^0.13.0",
    "ioredis": "^5.8.0",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "mongoose": "^8.2.1",
    "mongoose-aggregate-paginate-v2": "^1.0.7",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.0",
    "otp-generator": "^4.0.1",
    "pdf-parse": "^1.1.1",
    "pptxgenjs": "^3.12.0",
    "rate-limit-redis": "^4.2.2",
    "razorpay": "^2.9.2",
    "rimraf": "^6.0.1",
    "socket.io": "^4.8.1",
    "tailwindcss": "^4.1.17",
    "uuid": "^11.0.5",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "xss": "^1.0.14"
  }
}
